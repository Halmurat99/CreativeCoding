void setup(){
  size(800,600);
  background(255);
  noFill();
  frameRate(1);
}

void draw(){
  background(255);
  for(int i=10;i<width;i+=random(20,100)){
    ellipse(i,height/2,20,height);
  }
  for(int j=0;j<height;j+=random(20,100)){
    ellipse(width/2,j,width,20);
  }
  for(int k=0;k<width;k+=random(10,50)){
    ellipse(width/2,height/2,k,k);
  }
}
  
